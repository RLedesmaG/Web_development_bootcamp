<%- include('partials/header') -%>

 <h1>Compose</h1>
 <form class="" action="/compose" method="post">
   <div class="form-group">
     <label for="postTitle">Title</label>
     <input class='form-control' id='postTitle' type="text" name="newTitle" value="">
     <label for="postBody">Post</label>
     <textarea class='form-control' id='postBody' name="newBody" rows="8" cols="80"></textarea>
   </div>
   <button class="btn btn-primary" type="submit" name="button">Publish</button>
 </form>


<%- include('partials/footer') -%>
